{
	"name": "raphievila/xtags",
	"version": "dev-master",
	"description": "A simple way to create HTML tag directly into PHP code, no code breaks.",
	"repositories": {
		"url": "https://github.com/raphievila/xtags.git",
		"type": "https",
		"reference": "master"
	},
	"license": "GNU 3.0",
	"require": {
		"php": "^5.3 || ^7.0"
	},
	"autoload": {
		"psr-4": {
			"xTags\\": "src/",
			"Vendor\\Namespace\\": ""
		},
		"classmap": ["src/"]
	},
	"config": {
		"github-protocols": ["git", "https", "http"]
	},
	"minimum-stability": "dev"
}
